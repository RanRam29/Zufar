{
  "version": 2,
  "builds": [{ "src": "package.json", "use": "@vercel/static-build" }],
  "framework": "vite",
  "rewrites": [
    { "source": "/(.*)", "destination": "/" }  
  ],
  "headers": [
    {
      "source": "/:all*\\.(js|css|svg|png|jpg|jpeg|ico|txt)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}
